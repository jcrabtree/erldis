{erl_first_files, ["src/gen_server2.erl"]}.
{app,[erldis]}.
{erl_opts, [warn_unused_vars,
            warn_export_all,
            warn_shadow_vars,
            warn_unused_import,
            warn_unused_function,
            warn_bif_clash,
            warn_unused_record,
            warn_deprecated_function,
            warn_obsolete_guard,
            strict_validation,
            warn_export_vars,
            warn_exported_vars,
            debug_info, {i, "include"}, {src_dirs, ["src"]}]}.
{eunit_compile_opts, [{src_dirs, ["test"]}]}.
{cover_enabled, true}.
{clean_files, ["ebin/*.beam", "priv/log/*", "rel/*"]}.
{target, "rel"}.
{xref_checks, [undefined_function_calls]}.